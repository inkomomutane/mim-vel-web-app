{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "algolia/algoliasearch-client-php": "^2.2",
        "awssat/laravel-visits": "^3.0",
        "beyondcode/laravel-websockets": "^1.12",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "jorenvanhocht/laravel-share": "^4.1",
        "laravel/framework": "^7.0",
        "laravel/scout": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "2.4",
        "livewire/livewire": "^2.5",
        "nicolaslopezj/searchable": "1.*",
        "overtrue/laravel-lang": "^5.0",
        "pusher/pusher-php-server": "~3.0",
        "spatie/laravel-permission": "^4.2",
        "spatie/laravel-searchable": "^1.9"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^4.5",
        "mockery/mockery": "^1.3.1",
        "mpociot/laravel-test-factory-helper": "^2.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "reliese/laravel": "^1.1",
        "thedoctor0/laravel-factory-generator": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
