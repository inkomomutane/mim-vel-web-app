{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "awssat/laravel-visits": "^3.0",
        "blade-ui-kit/blade-ui-kit": "^0.3.4",
        "codeat3/blade-fluentui-system-icons": "^1.24",
        "codeat3/blade-radix-icons": "^1.5",
        "doctrine/dbal": "2.x",
        "guzzlehttp/guzzle": "^7.2",
        "jorenvanhocht/laravel-share": "^4.2",
        "laravel/framework": "^9.11",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "livewire/livewire": "^2.10",
        "overtrue/laravel-lang": "~6.0",
        "ralphjsmit/laravel-seo": "^1.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-medialibrary": "^10.3",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-sitemap": "^6.1",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-tags": "^4.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^5.2",
        "laravel-lang/lang": "^10.9",
        "laravel-lang/publisher": "^13.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "reliese/laravel": "^1.1",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-web-tinker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
            "@php artisan optimize"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
